.agreementWrap{background:#fff;}

/*个人中心*/
.centerWarp{padding-bottom:60px;}
.u-top {background: url(../../assets/images/userTop.png) 0 0 no-repeat;background-size: cover;padding: 50px 0 37px;position:relative;}
.u-top-wrap {width: 230px;margin: 0 auto;text-align: center;}
.u-top-setting {width: 24px;height: 24px;background: url(../../assets/images/setting.png) 0 0 no-repeat;background-size: 24px;margin-top: 25px;position:absolute;top:-10px;left:13px;}
.u-top-user {display: inline-block;color: #fff;position:relative;}
.u-top-wrap .imgbox {width: 75px;display:block;position:relative;margin:0 auto 15px;}
.u-top-wrap .imgbox:before{content:'';display:block;padding:50% 0;}
.u-top-wrap .imgbox img{width:100%;height:100%;position:absolute;top:0;left:0;border-radius: 50%;}
.u-top-wrap span {position: relative;}
.u-top-msg {width: 40px;height: 40px;position:absolute;top:-20px;right:0px;margin-top:25px;}
.u-top-msg i{padding:0 13px;height:100%;width:25px;background:url(../../assets/images/white_news.png) no-repeat center center;background-size:25px;display:block;position:absolute;right:0;top:0;}
.u-top-msg .dot{height:6px;width:6px;background:#e54749;display:block;position:absolute;right:15px;top:8px;border-radius:15px;border:1px solid #fff;}
.u-top-user .bt{width:75px;line-height:22px;font-size:12px;text-align:center;color:#333;background:#ffec1b;position:absolute;bottom:25px;left:50%;transform:translateX(-50%);-webkit-transform:translateX(-50%);}
.u-top-user .bt a{display:block;}
.ysecline{background:#fff;}
.ym-setboxsec {padding: 0 10px;}
.ym-setboxsec .title {height:35px;line-height: 35px;position: relative;border-bottom: 1px solid #e1e1e1;padding-right: 20px;color: #333333;font-size: 16px;display: block;}
.ym-setboxsec .title span{line-height:35px;float:left;}
.ym-setboxsec .title p{line-height:35px;float:right;font-size:14px;}
.ym-setboxsec .title i{display:block;width:30px;height:100%;background: url(../../assets/images/back.png) no-repeat center center;background-size: 7px;position: absolute;top: 50%;right: -8px;transform: rotateY(180deg) translateY(-50%);-webkit-transform: rotateY(180deg) translateY(-50%);}
.ymysetbox-tab {padding-bottom: 10px;border-bottom:1px solid #e4e4e4;}
.ymysetbox-tab .ybox {display: block;float: left;width: 25%;text-align: center;box-sizing: border-box;padding-top: 10px;color: #666;}
.ymysetbox-tab .ybox .icondiv {padding: 0 20px;margin-bottom: 2px;}
.ymysetbox-tab .icon {width: 100%;padding-bottom: 100%;display: block;position: relative;}
.ymysetbox-tab .ybox:nth-child(1) .icon {background: url(../../assets/images/to-pay.png) center center no-repeat;background-size: 30px;}
.ymysetbox-tab .ybox:nth-child(2) .icon {background: url(../../assets/images/to-ship.png) center center no-repeat;background-size: 22px;}
.ymysetbox-tab .ybox:nth-child(3) .icon {background: url(../../assets/images/to-receive.png) center center no-repeat;background-size: 40px;}
.ymysetbox-tab .ybox:nth-child(4) .icon {background: url(../../assets/images/to-comment.png) center center no-repeat;background-size: 25px;}
.p_cen_m ul li {background: #fff;padding: 2% 35px 2% 3%;line-height: 35px;overflow: hidden;position: relative;font-size: 15px;}
.p_cen_m ul li em {display: inline-block;width: 30px;height: 30px;vertical-align: middle;}
.p_cen_m ul li .ico1 {background: url(../../assets/images/ico1.png) center center no-repeat;background-size: 20px;}
.p_cen_m ul li .ico2 {background: url(../../assets/images/ico2.png) center center no-repeat;background-size: 20px;}
.p_cen_m ul li .ico3 {background: url(../../assets/images/ico3.png) center center no-repeat;background-size: 20px;}
.p_cen_m ul li .ico4 {background: url(../../assets/images/ico4.png) center center no-repeat;background-size: 20px;}
.p_cen_m ul li .ico5 {background: url(../../assets/images/ico5.png) center center no-repeat;background-size: 20px;}
.p_cen_m ul li .ico6 {background: url(../../assets/images/ico6.png) center center no-repeat;background-size: 20px;}
.p_cen_m ul li .ico7 {background: url(../../assets/images/ico7.png) center center no-repeat;background-size: 20px;}
.p_cen_m ul li span {display: inline-block;line-height: 35px;}
.p_cen_m ul li .s1 {color: #666;margin-left: 4%;}
.p_cen_m ul li .ei {background: url(../../assets/images/back.png) no-repeat center center;background-size: 7px;position: absolute;top: 50%;right: 5px;transform: rotateY(180deg) translateY(-50%);-webkit-transform: rotateY(180deg) translateY(-50%);}
.ym-duipeng{padding:10px 0;overflow:hidden;border-bottom:1px solid #e4e4e4;}
.ym-duipeng .duibox{width:33%;display:block;color:#666;text-align:center;float:left;font-size:16px;}
.ym-duipeng .duibox span{color:#e54749;display:block;overflow:hidden;text-overflow:ellipsis;white-space:nowrap;}
.u-top-nologin{text-align:center;padding:5% 0;}
.u-top-nologin a{display:inline-block;line-height:35px;text-align:center;color:#e54749;background:#fff;margin:0 7%;width:25%;border-radius:4px;}
.ymysetbox-tab .ybox .icondiv .icon em{display:block;width:15px;height:15px;background:#e54749;color:#fff;font-size:10px;line-height:15px;text-align:center;position:absolute;top:5%;right:0;border-radius:15px;overflow:hidden;}
.ymysetbox-tab .ybox .icondiv .icon em b{transform:scale(0.75);-webkit-transform:scale(0.75);display:block;}