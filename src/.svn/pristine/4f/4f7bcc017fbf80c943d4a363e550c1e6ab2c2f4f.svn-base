/*底部固定菜单*/
.y-ycar-foot {height: 50px;background: #fff;bottom: 0;position: fixed;left: 0;right: 0;}
.y-ycar-foot a {width: 25%;display: block;height: 100%;float: left;position:relative;}
.y-ycar-foot a .icon {display: block;padding-top: 30px;height: 20px;line-height: 20px;color: #817d7d;font-size: 12px;text-align: center;position: relative;}
.y-ycar-foot .icon i {position: absolute;top: 8px;left: 50%;right: 0;display: block;width: 22px;height: 22px;margin-left: -11px;}
.y-ycar-foot a:nth-child(1) i {background: url(../images/index_ico.png) center center no-repeat;background-size: contain;}
.y-ycar-foot a:nth-child(2) i {background: url(../images/classfiy_ico.png) center center no-repeat;background-size: contain;}
.y-ycar-foot a:nth-child(3) i {background: url(../images/shopcar_ico.png) center center no-repeat;background-size: contain;}
.y-ycar-foot a:nth-child(4) i {background: url(../images/my_ico.png) center center no-repeat;background-size: contain;}
.y-ycar-foot a.on .icon {color: #e54749;}
.y-ycar-foot a.on:nth-child(1) i {background:url(../images/index_on_ico.png) center center no-repeat;background-size:contain;}
.y-ycar-foot a.on:nth-child(2) i {background:url(../images/classfiy_on_ico.png) center center no-repeat;background-size:contain;}
.y-ycar-foot a.on:nth-child(3) i {background:url(../images/shopcar_on_ico.png) center center no-repeat;background-size:contain;}
.y-ycar-foot a.on:nth-child(4) i {background:url(../images/my_on_ico.png) center center no-repeat;background-size:contain;}

/*过渡动画*/
.fade-slide-enter-active, .fade-slide-leave-active {
